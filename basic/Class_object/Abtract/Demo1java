abstract class A {
    public void f1() {
        System.out.println("A.f1()");
    }

    public void f2() {
        System.out.println("A.f2()");
    }
}

public class Demo1 {
    public static void main(String[] args) {
        A a = new A() {
        };
        a.f1();

        int s = 0;
        for (int i = 0; i < 10; i++) {
            s += i;
        }
        System.out.println(s);
    }
}
